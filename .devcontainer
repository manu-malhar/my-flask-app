{
  "name": "Flask Docker Container",
  "dockerFile": "Dockerfile",
  "forwardPorts": [5000],
  "portsAttributes": {
    "5000": {
      "label": "Flask Application",
      "onAutoForward": "openPreview"
    }
  },
  "postCreateCommand": "pip install -r requirements.txt",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-azuretools.vscode-docker"
      ]
    }
  }
}